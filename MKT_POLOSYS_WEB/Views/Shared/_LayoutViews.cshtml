    <link rel="stylesheet" href="~/lib/vendor/sweat-alert/sweetalert.css" />*@
    <script src="~/lib/vendor/sweat-alert/sweetalert2.all.min.js"></script>
        <script src="~/lib/vendor/sweat-alert/sweetalert2.js"></script>
        <link rel="stylesheet" href="~/lib/vendor/sweat-alert/sweetalert2.css" />



    <script src="~/js/taskInquiryview.js"></script>
    <script src="~/lib/vendor/dist/js/custom.min.js"></script>
    @RenderSection("Scripts", required: false)
</body>

</html>